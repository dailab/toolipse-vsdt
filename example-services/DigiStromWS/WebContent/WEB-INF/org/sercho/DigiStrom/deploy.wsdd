<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from DigiStromService WSDL service -->

  <service name="DigiStromServiceSOAP" provider="java:RPC" style="rpc" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://sercho.org/DigiStrom/"/>
      <parameter name="wsdlServiceElement" value="DigiStromService"/>
      <parameter name="wsdlServicePort" value="DigiStromServiceSOAP"/>
      <parameter name="className" value="org.sercho.DigiStrom.DigiStromServiceSOAPSkeleton"/>
      <parameter name="wsdlPortType" value="DigiStromPT"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <parameter name="allowedMethods" value="*"/>

  </service>
</deployment>

<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<wsdl:definitions xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/" xmlns:tns="http://sercho.org/WeatherProxyWS/" xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/" xmlns:xsd="http://www.w3.org/2001/XMLSchema" name="WeatherProxyWS" targetNamespace="http://sercho.org/WeatherProxyWS/">

  <wsdl:message name="GetWeatherRequest">
  	<wsdl:part name="city" type="xsd:string"/>
  	<wsdl:part name="country" type="xsd:string"/>
  </wsdl:message>
  
  <wsdl:message name="GetWeatherResponse">
    <wsdl:part name="weatherdata" type="xsd:string"/>
  </wsdl:message>
  
  <wsdl:portType name="WeatherProxyPT">
    <wsdl:operation name="GetWeather">
      <wsdl:input message="tns:GetWeatherRequest"/>
      <wsdl:output message="tns:GetWeatherResponse"/>
    </wsdl:operation>
  </wsdl:portType>

  <wsdl:binding name="WeatherProxySOAP" type="tns:WeatherProxyPT">
  	<soap:binding style="rpc" transport="http://schemas.xmlsoap.org/soap/http"/>
  	<wsdl:operation name="GetWeather">
  		<soap:operation soapAction="http://sercho.org/WeatherProxyWS/GetWeather"/>
  		<wsdl:input>
	 		<soap:body namespace="http://sercho.org/WeatherProxyWS/" use="literal"/>
  		</wsdl:input>
  		<wsdl:output>
	 		<soap:body namespace="http://sercho.org/WeatherProxyWS/" use="literal"/>
  		</wsdl:output>
  	</wsdl:operation>
  </wsdl:binding>
  
  <wsdl:service name="WeatherProxyService">
    <wsdl:port binding="tns:WeatherProxySOAP" name="WeatherProxyServiceSOAP">
      <soap:address location="http://localhost:8080/WeatherProxyWS/services/WeatherProxyServiceSOAP"/>
    </wsdl:port>
  </wsdl:service>
  
</wsdl:definitions>

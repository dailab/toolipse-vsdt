<?xml version="1.0" encoding="ASCII"?>
<bpws:process xmlns:bpws="http://schemas.xmlsoap.org/ws/2003/03/business-process/" xmlns:ns1="http://Mathematican_Faculty/WSDL/" xmlns:xsd="http://www.w3.org/2001/XMLSchema" abstractProcess="no" enableInstanceCompensation="no" expressionLanguage="http://www.w3.org/TR/1999/REC-xpath-19991116" name="Mathematican_Faculty" queryLanguage="http://www.w3.org/TR/1999/REC-xpath-19991116" suppressJoinFailure="no" targetNamespace="http://Mathematican_Faculty">
  <bpws:partnerLinks>
    <bpws:partnerLink myRole="Mathematican_myRole" name="Mathematican" partnerLinkType="ns1:Mathematican_PLT" partnerRole="Mathematican_partnerRole"/>
  </bpws:partnerLinks>
  <bpws:variables>
    <bpws:variable messageType="ns1:GetFacultyRequest_MessageDataMessage" name="GetFacultyRequest_MessageData"/>
    <bpws:variable messageType="ns1:GetFacultyResponse_MessageDataMessage" name="GetFacultyResponse_MessageData"/>
    <bpws:variable messageType="ns1:Mathematican_ProcessDataMessage" name="Mathematican_ProcessData"/>
  </bpws:variables>
  <bpws:sequence>
    <bpws:receive name="start" createInstance="yes" operation="Faculty" partnerLink="Mathematican" portType="ns1:TestService" variable="GetFacultyRequest_MessageData"/>
    <bpws:assign name="start_endAssignments">
      <bpws:copy>
        <bpws:from expression="bpws:getVariableData('GetFacultyRequest_MessageData','param')"/>
        <bpws:to part="n" variable="Mathematican_ProcessData"/>
      </bpws:copy>
      <bpws:copy>
        <bpws:from expression="1"/>
        <bpws:to part="p" variable="Mathematican_ProcessData"/>
      </bpws:copy>
    </bpws:assign>
    <bpws:switch>
      <bpws:case condition="bpws:getVariableData('Mathematican_ProcessData','n') >= 0">
        <bpws:sequence>
          <bpws:empty name="__DO_NOTHING__"/>
          <bpws:while name="_greaterthanone_loop" condition="bpws:getVariableData('Mathematican_ProcessData','n') > 1">
            <bpws:sequence>
              <bpws:assign name="Multiply_startAssignments">
                <bpws:copy>
                  <bpws:from expression="bpws:getVariableData('Mathematican_ProcessData','p') * bpws:getVariableData('Mathematican_ProcessData','n')"/>
                  <bpws:to part="p" variable="Mathematican_ProcessData"/>
                </bpws:copy>
                <bpws:copy>
                  <bpws:from expression="bpws:getVariableData('Mathematican_ProcessData','n') - 1"/>
                  <bpws:to part="n" variable="Mathematican_ProcessData"/>
                </bpws:copy>
              </bpws:assign>
              <bpws:empty name="Multiply"/>
              <bpws:empty name="__DO_NOTHING__"/>
            </bpws:sequence>
          </bpws:while>
          <bpws:assign name="_startAssignments">
            <bpws:copy>
              <bpws:from expression="bpws:getVariableData('Mathematican_ProcessData','p')"/>
              <bpws:to part="result" variable="GetFacultyResponse_MessageData"/>
            </bpws:copy>
          </bpws:assign>
          <bpws:reply name="" operation="Faculty" partnerLink="Mathematican" portType="ns1:TestService" variable="GetFacultyResponse_MessageData"/>
        </bpws:sequence>
      </bpws:case>
      <bpws:otherwise>
        <bpws:sequence>
          <bpws:assign name="_startAssignments">
            <bpws:copy>
              <bpws:from expression="0 - 1"/>
              <bpws:to part="result" variable="GetFacultyResponse_MessageData"/>
            </bpws:copy>
          </bpws:assign>
          <bpws:reply name="" operation="Faculty" partnerLink="Mathematican" portType="ns1:TestService" variable="GetFacultyResponse_MessageData"/>
        </bpws:sequence>
      </bpws:otherwise>
    </bpws:switch>
  </bpws:sequence>
</bpws:process>
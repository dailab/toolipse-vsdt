<?xml version="1.0" encoding="ASCII"?>
<wsdl:definitions xmlns:plnk="http://docs.oasis-open.org/wsbpel/2.0/plnktype" xmlns:soap="http://schemas.xmlsoap.org/wsdl/soap/" xmlns:tns="http://AnomalyDetection/WSDL/" xmlns:wsdl="http://schemas.xmlsoap.org/wsdl/" xmlns:xsd="http://www.w3.org/2001/XMLSchema" targetNamespace="http://AnomalyDetection/WSDL/">
  <plnk:partnerLinkType name="SHEA_PLT">
    <plnk:role name="SHEA_myRole" portType="tns:AnomalyDetectionPT"/>
  </plnk:partnerLinkType>
  <plnk:partnerLinkType name="Logger_PLT">
    <plnk:role name="Logger_partnerRole" portType="tns:LoggingPT"/>
  </plnk:partnerLinkType>
  <plnk:partnerLinkType name="AnomalySensor_PLT">
    <plnk:role name="AnomalySensor_partnerRole" portType="tns:SensorInterface"/>
  </plnk:partnerLinkType>
  <plnk:partnerLinkType name="Notificator_PLT">
    <plnk:role name="Notificator_partnerRole" portType="tns:MailAndSMSPT"/>
  </plnk:partnerLinkType>
  <wsdl:message name="ADStartMessage_MessageDataMessage">
    <wsdl:part name="paramSMS" type="xsd:string"/>
    <wsdl:part name="paramEMail" type="xsd:string"/>
    <wsdl:part name="paramInterval" type="xsd:integer"/>
    <wsdl:part name="paramThreshold" type="xsd:float"/>
    <wsdl:part name="paramMaxAlarm" type="xsd:integer"/>
  </wsdl:message>
  <wsdl:message name="logRequest_MessageDataMessage">
    <wsdl:part name="message" type="xsd:string"/>
    <wsdl:part name="level" type="xsd:integer"/>
  </wsdl:message>
  <wsdl:message name="getLastStatusRequest_MessageDataMessage">
    <wsdl:part name="timeframe" type="xsd:long"/>
  </wsdl:message>
  <wsdl:message name="getLastStatusResponse_MessageDataMessage">
    <wsdl:part name="status" type="xsd:double"/>
  </wsdl:message>
  <wsdl:message name="SendSMSRequest_MessageDataMessage">
    <wsdl:part name="to" type="xsd:string"/>
    <wsdl:part name="text" type="xsd:string"/>
  </wsdl:message>
  <wsdl:message name="SendSMSResponse_MessageDataMessage">
    <wsdl:part name="result" type="xsd:boolean"/>
  </wsdl:message>
  <wsdl:message name="SendMailRequest_MessageDataMessage">
    <wsdl:part name="to" type="xsd:string"/>
    <wsdl:part name="text" type="xsd:string"/>
  </wsdl:message>
  <wsdl:message name="SendMailResponse_MessageDataMessage">
    <wsdl:part name="result" type="xsd:boolean"/>
  </wsdl:message>
  <wsdl:message name="ADStopMessage_MessageDataMessage"/>
  <wsdl:message name="ADProcess_ProcessDataMessage">
    <wsdl:part name="inInterval" type="xsd:string"/>
    <wsdl:part name="inThreshold" type="xsd:double"/>
    <wsdl:part name="inMaxAlarm" type="xsd:integer"/>
    <wsdl:part name="inSMS" type="xsd:string"/>
    <wsdl:part name="inEMail" type="xsd:string"/>
    <wsdl:part name="tmpAnomLevel" type="xsd:double"/>
    <wsdl:part name="tmpAlarmLevel" type="xsd:integer"/>
    <wsdl:part name="tmpNotification" type="xsd:string"/>
  </wsdl:message>
  <wsdl:portType name="AnomalyDetectionPT">
    <wsdl:operation name="start">
      <wsdl:input message="tns:ADStartMessage_MessageDataMessage"/>
    </wsdl:operation>
    <wsdl:operation name="stop">
      <wsdl:input message="tns:ADStopMessage_MessageDataMessage"/>
    </wsdl:operation>
  </wsdl:portType>
  <wsdl:portType name="LoggingPT">
    <wsdl:operation name="log">
      <wsdl:input message="tns:logRequest_MessageDataMessage"/>
    </wsdl:operation>
  </wsdl:portType>
  <wsdl:portType name="SensorInterface">
    <wsdl:operation name="getLastStatus">
      <wsdl:input message="tns:getLastStatusRequest_MessageDataMessage"/>
      <wsdl:output message="tns:getLastStatusResponse_MessageDataMessage"/>
    </wsdl:operation>
  </wsdl:portType>
  <wsdl:portType name="MailAndSMSPT">
    <wsdl:operation name="SendSMS">
      <wsdl:input message="tns:SendSMSRequest_MessageDataMessage"/>
      <wsdl:output message="tns:SendSMSResponse_MessageDataMessage"/>
    </wsdl:operation>
    <wsdl:operation name="SendMail">
      <wsdl:input message="tns:SendMailRequest_MessageDataMessage"/>
      <wsdl:output message="tns:SendMailResponse_MessageDataMessage"/>
    </wsdl:operation>
  </wsdl:portType>
</wsdl:definitions>


service Looping ( in $param1 : int ) ( out $result1 : int ) {
    var $n : int ;
    {
        {
            $n = $param1 ;
        }
        if ( $n == 1 ) {
            $n = $n + 1 ;
            {
                var $loop1_loopCounter : int ;
                $loop1_loopCounter = 0 ;
                while ( $loop1_loopCounter < 9 ) {
                    $loop1_loopCounter = $loop1_loopCounter + 1 ;
                    print "Executing Loop1" ;
                }
            }
        }
        else {
            if ( $n == 2 ) {
                $n = $n + 1 ;
                {
                    while ( $n < 20 ) {
                        print "Executing Loop2" ;
                    }
                }
            }
            else {
                if ( $n == 3 ) {
                    {
                        {
                            $n = $n + 1 ;
                            print "Executing Loop3a" ;
                        }
                        while ( ! ( $n == 30 ) ) {
                            {
                                print "Executing Loop3b" ;
                            }
                            {
                                $n = $n + 1 ;
                                print "Executing Loop3a" ;
                            }
                        }
                    }
                }
            }
        }
        {
            $result1 = $n ;
        }
    }
}
   </extension>

	<extension point="org.eclipse.ui.views.properties.tabbed.propertySections">
      <propertySections contributorId="de.dailab.vsdt.diagram.meta">
         <propertySection
               class="de.dailab.vsdt.diagram.properties.IdObjectSection"
               filter="de.dailab.vsdt.diagram.properties.IdObjectSection"
               id="de.dailab.vsdt.diagram.properties.IdObjectSection"
               tab="de.dailab.vsdt.diagram.properties.category">
         </propertySection>
         <propertySection
